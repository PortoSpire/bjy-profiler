{
    "name": "andrey-mokhov/bjy-profiler",
    "description": "Database profiler for Zend\\Db (also plugin for ZendDeveloperTools). Module forked from bjyoungblood/bjy-profiler",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "zf3",
        "db",
        "profiler",
        "zdt"
    ],
    "homepage": "https://github.com/andrey-mokhov/BjyProfiler",
    "authors": [
        {
            "name": "Ben Youngblood",
            "email": "bx.youngblood@gmail.com",
            "homepage": "http://bjyoungblood.com/"
        },
        {
            "name": "Andrey N. Mokhov",
            "email": "github@mokhov.com",
            "homepage": "https://github.com/andrey-mokhov"
        }
    ],
    "require": {
        "php": "^5.5 || ^7.0",
        "zendframework/zend-servicemanager": "^3.0.3",
        "zendframework/zend-modulemanager": "^2.7",
        "zendframework/zend-db": "^2.7",
        "zendframework/zend-log": "^2.7"
    },
    "autoload": {
        "psr-4": {
            "BjyProfiler\\": "src/"
        }
    },
    "extra": {
        "zf": {
            "module": "BjyProfiler",
            "config-provider": "BjyProfiler\\ConfigProvider"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0",
        "zendframework/zend-mvc": "^2.7 || ^3.0"
    }
}
